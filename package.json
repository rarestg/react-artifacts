{
  "name": "react-artifacts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "biome check --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "generate-types": "wrangler types",
    "typecheck": "npm run generate-types && tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.worker.json",
    "typecheck:worker": "npm run generate-types && tsc --noEmit -p tsconfig.worker.json",
    "preview": "vite preview",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@cloudflare/vite-plugin": "^1.22.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "wrangler": "^4.61.1"
  }
}
